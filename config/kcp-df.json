{
    // 监听端口
    "listen": ":45000",
    // 连接目标，含端口
    "target": "KCP_TARGET",
    // KCP密码
    "key": "KCP_PWD",
    // 加密方式
    "crypt": "KCP_CR",
    // 加速模式
    "mode": "KCP_MODE",
    // MTU
    "mtu": KCP_MTU,
    // 发送窗口大小
    "sndwnd": KCP_SW,
    // 接收窗口大小
    "rcvwnd": KCP_RW,
    // 数据包
    "datashard": KCP_DS,
    // 校验包
    "parityshard": KCP_PS,
    // DSCP
    "dscp": KCP_SDSCP,
    // 禁用压缩
    "nocomp": KCP_NCP,
    // 立即响应
    "acknodelay": false,
    // 无延迟模式
    "nodelay": 0,
    // 协议内部工作间隔
    "interval": 40,
    // 快速重传
    "resend": 0,
    // 关闭流控，1关闭
    "nc": 0,
    // 每个接口的缓冲大小
    "sockbuf": KCP_BUF,
    // 心跳时间，秒
    "keepalive": KCP_KAL,
    // 模拟TCP连接
    "tcp": KCP_TCP,
    // 日志文件路径
    "log": "/root/hmss/kcp.log",
    // 静默模式
    "quiet": true
}